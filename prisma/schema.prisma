generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id            Int     @id @map("_id")
  messages      Int     @default(0)
  firstname     String
  lastname      String?
  username      String?
  faculty       String?
  educationForm String?
  course        String?
  studyGroup    String?
}

model EmployeeCache {
  id        String     @id @default(auto()) @map("_id") @db.ObjectId
  date      DateTime
  employees Employee[]
}

type Employee {
  name  String
  key   String
}
