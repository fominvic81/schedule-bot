generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id            Int     @id @map("_id")
  firstname     String
  lastname      String?
  username      String?
  faculty       String?
  educationForm String?
  course        String?
  studyGroup    String?
}
